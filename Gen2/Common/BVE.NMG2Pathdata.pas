  FIDS := TStringList.Create;
  FItems := TObjectlist<TPathData>.Create(True);
  LoadDefaults;
end;

destructor TG2PathDataList.Destroy;
begin
  FIDS.Free;
  FItems.Free;
  inherited;
end;

procedure TG2PathDataList.LoadDefaults;
var i : integer;
    PathData : TPathData;
begin
  FItems.Clear;
  FIDS.Clear;
  for i := 0 to 69 do begin
    PathData := TPathData.Create;
    PathData.Data := G2SymbolData[i];

    FItems.Add(PathData);
    FIDS.AddObject(IntToStr(i), PathData);
  end;
end;

procedure TG2PathDataList.LoadFromFile(const aFilename: string);
var FSymbols : TXMLDocument;
    XMLSVGType : IXMLSvgType;
    XMLDefsType : IXMLDefsType;
    XMLPathTypeList : IXMLPathTypeList;
    PathData : TPathData;
    i : integer;
begin
  FSymbols := TXMLDocument.Create(self);
  try
    FSymbols.FileName := aFileName;
    FSymbols.Active := True;

    FItems.Clear;
    FIDS.Clear;
    try
      XMLSVGType := Getsvg(FSymbols);
      XMLDefsType := XMLSVGType.Defs as IXMLDefsType;

      XMLPathTypeList := XMLDefsType.Path as IXMLPathTypeList;
      for i := 0 to XMLPathTypeList.Count - 1 do begin
        PathData := TPathData.Create;
        PathData.Data := XMLPathTypeList.Items[i].D;

        FItems.Add(PathData);
        FIDS.AddObject(XMLPathTypeList.Items[i].Id, PathData);
      end;
    finally
      FSymbols.Active := False;
    end;
  finally
    FSymbols.Free;
  end;
end;

procedure TG2PathDataList.SaveToFile(const aFilename: string);
var FSymbols : TXMLDocument;
    XMLSVGType : IXMLSvgType;
    XMLDefsType : IXMLDefsType;
    XMLPathType : IXMLPathType;
    XMLGType : IXMLGType;
    XMLUseType : IXMLUseType;
    i : integer;
    Rect : TRectF;
    PathData : TPathData;
begin
  FSymbols := TXMLDocument.Create(self);
  try
    FSymbols.Active := True;

    XMLSVGType :=  Getsvg(FSymbols);
    XMLSVGType.DeclareNamespace('svg', 'http://www.w3.org/2000/svg');
    XMLSVGType.DeclareNamespace('xlink', 'http://www.w3.org/1999/xlink');

    XMLDefsType := XMLSVGType.AddChild('defs') as IXMLDefsType;

    for i := 0 to High(G2SymbolData) do begin
      PathData := TPathData.Create;
      try
        PathData.Data := G2SymbolData[ i];

        Rect := PathData.GetBounds;
        if (Rect.Width = 0) or (Rect.Height = 0) then
          exit;

        PathData.Translate( -Rect.Left, -Rect.Top);

        XMLPathType := XMLDefsType.AddChild('path') as IXMLPathType;
        XMLPathType.Id := IntToStr(i);
        XMLPathType.D := PathData.Data;
      finally
        PathData.Free;
      end;
    end;

    XMLGType := XMLSVGType.AddChild('g') as IXMLGType;
    XMLGType.Id := 'group_1';


    for i := 0 to High(G2SymbolData) do begin
      XMLUseType := XMLGType.AddChild('use') as IXMLUseType;
      XMLUseType.Id := 'use_' + IntToStr(i);
      XMLUseType.X := (i mod 10) * 40;
      XMLUseType.Y := trunc(i / 10) * 40;
      XMLUseType.Href := '#' + IntToStr(i);
    end;

    FSymbols.SaveToFile(aFileName);
    FSymbols.Active := False;
  finally
    FSymbols.Free;
  end;
end;

procedure TG2PathDataList.DrawSymbol( aCanvas : TCanvas; aDestRect : TRectF; aImageID : string);
var Rect : TRectF;
    PathData : TPathData;
    dx, dy, s, dx2, dy2 : single;
    SaveMatrix : TMatrix;
    i : integer;
begin
  i := FIDS.IndexOf(aImageID);
  if i < 0 then
    exit;

  PathData := FIDS.Objects[i] as TPathData;

  Rect := PathData.GetBounds;
  if (Rect.Width = 0) or (Rect.Height = 0) then
    exit;

  dx := (Rect.Left + Rect.Width / 2);
  dy := (Rect.Top + Rect.Height / 2);

  dx2 := (aDestRect.Left + (aDestRect.Right - aDestRect.Left)/2);
  dy2 := (aDestRect.Top + (aDestRect.Bottom - aDestRect.Top)/2);

  s := 0.7 *  System.Math.min(aDestRect.Width / Rect.Width, aDestRect.Height / Rect.Height);

  SaveMatrix := aCanvas.Matrix;
  try
    aCanvas.SetMatrix(
            MatrixMultiply( CreateTranslateMatrix(-dx, -dy),
            MatrixMultiply( CreateScaleMatrix(s, s),
            MatrixMultiply( CreateTranslateMatrix( dx2, dy2), SaveMatrix))));

    aCanvas.FillPath( PathData, 1);
  finally
    aCanvas.SetMatrix(SaveMatrix);
  end;
end;


initialization
  PathDataList := TG2PathDataList.Create(nil);

finalization
  PathDataList.Free;

end.
